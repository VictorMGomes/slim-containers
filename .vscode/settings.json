{
    "files.associations": {
        ".env.example": "dotenv",
        "*.env": "dotenv",
        "*.sh": "shellscript",
        "*.nginx": "nginx",
        "*conf.template": "nginx",
        "Dockerfile*": "dockerfile",
        "Dockerfile-*": "dockerfile",
        "compose.yml": "dockercompose",
        "*.md": "markdown"
    },
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format",
        "editor.formatOnSave": true
    },
    "shellcheck.executablePath": "",
    "shellcheck.run": "onSave"
}
